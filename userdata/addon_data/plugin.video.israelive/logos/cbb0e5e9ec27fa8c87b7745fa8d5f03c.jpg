$(window).ready(function(){
	$currentpage=1;
	$('.turn-page').click(function(){
		
		$number=parseInt($(this).attr('id'));
		$('.page-number').removeClass('on');
		$pos=($number-1)*720;
		if($number>$currentpage)
		{
			$('.home-posts-container').animate({left:'-'+$pos+'px'}, 500, function(){});
			$(this).parent().addClass('on');
		}
		else if ($number<$currentpage)
		{
			$('.home-posts-container').animate({left:'-'+$pos+'px'}, 500, function(){});
			$(this).parent().addClass('on');
		}
		$currentpage=$number;
	});
	$('.fancybox-media').fancybox({
		openEffect  : 'none',
